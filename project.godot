config_version=4

_global_script_classes=[ {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
} ]
_global_script_class_icons={
"GitAPI": ""
}

[application]

config/name="Unfound Memories"
config/description="This is just the beginning"
run/main_scene="res://scenes/introduction.tscn"
run/low_processor_mode=true
boot_splash/image="res://sprites/splash.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://game_icon.png"
config/macos_native_icon="res://game_icon.icns"
config/windows_native_icon="res://game_icon.ico"

[debug]

settings/crash_handler/message=":( A game has crashed, Please restart the process."
settings/fps/force_fps=60

[display]

window/size/always_on_top=true
window/per_pixel_transparency/allowed=true
window/energy_saving/keep_screen_on=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

